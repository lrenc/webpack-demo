{
  "compilerOptions": {
    "target": "es2016",
    // 指定生成代码的模板标准
    "module": "commonjs",
    // 不允许隐式的 any 类型
    "noImplicitAny": true,
    // 删除注释
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}